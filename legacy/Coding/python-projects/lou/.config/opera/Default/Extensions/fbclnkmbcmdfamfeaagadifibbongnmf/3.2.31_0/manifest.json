{
   "author": "AppWork GmbH",
   "background": {
      "page": "index.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": "images/icon32.png",
      "default_popup": "popup.html",
      "default_title": "MyJDownloader"
   },
   "commands": {
      "auto-grab-downloads": {
         "description": "__MSG_toggle_auto_grab_links__",
         "suggested_key": {
            "default": "Ctrl+Shift+Y",
            "mac": "Command+Shift+Y"
         }
      },
      "toggle-clipboard-observer": {
         "description": "__MSG_toggle_clipboard_observer__",
         "suggested_key": {
            "default": "Ctrl+Shift+X",
            "mac": "Command+Shift+X"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "contentscripts/toolbarContentscript.js", "contentscripts/selectionContentscript.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   }, {
      "all_frames": true,
      "js": [ "contentscripts/rc2Contentscript.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "contentscripts/browserSolverEnhancer.js" ],
      "matches": [ "http://127.0.0.1/*/?id=*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "contentscripts/webinterfaceEnhancer.js" ],
      "matches": [ "http://my.jdownloader.org/*", "https://my.jdownloader.org/*", "http://my.jdownloader.org/*" ],
      "run_at": "document_end"
   } ],
   "default_locale": "en",
   "description": "__MSG_extensionDescription__",
   "developer": {
      "name": "appwork"
   },
   "homepage_url": "https://my.jdownloader.org",
   "icons": {
      "48": "images/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_extensionName__",
   "options_ui": {
      "page": "index.html#!/settings"
   },
   "permissions": [ "activeTab", "tabs", "storage", "webRequest", "webRequestBlocking", "contextMenus", "\u003Call_urls>" ],
   "short_name": "MyJDownloader",
   "update_url": "https://extension-updates.opera.com/api/omaha/update/",
   "version": "3.2.31",
   "web_accessible_resources": [ "toolbar.html", "autograbber-indicator.html", "res/browser_solver_template.html" ]
}
