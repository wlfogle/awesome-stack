{
  "settings": [
    {
      "provider": "cloudflare",
      "domain": "example.com",
      "host": "@",
      "ttl": 1,
      "token": "YOUR_CLOUDFLARE_TOKEN_HERE",
      "zone_identifier": "YOUR_ZONE_ID_HERE"
    }
  ],
  "backup": {
    "period": "24h",
    "directory": "/updater/data/backup"
  },
  "server": {
    "port": 8000,
    "address": "0.0.0.0"
  },
  "health": {
    "port": 9999,
    "address": "127.0.0.1"
  },
  "log": {
    "level": "info",
    "caller": false
  }
}
